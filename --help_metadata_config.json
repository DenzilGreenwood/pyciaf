{
  "storage_backend": "json",
  "storage_path": "--help_metadata",
  "enable_compression": false,
  "max_file_size_mb": 100,
  "enable_lazy_materialization": true,
  "batch_size": 100,
  "async_writes": true,
  "cache_size": 10000,
  "defer_validation": true,
  "fast_inference_mode": true,
  "memory_buffer_size": 50,
  "enable_deferred_lcm": true,
  "default_lcm_mode": "adaptive",
  "lcm_batch_size": 50,
  "lcm_processing_interval": 2.0,
  "lcm_queue_max_size": 10000,
  "lcm_immediate_threshold_ms": 50.0,
  "lcm_cpu_threshold_percent": 80.0,
  "lcm_memory_threshold_mb": 500.0,
  "lcm_enable_persistence": true,
  "lcm_storage_dir": "deferred_lcm_storage",
  "lcm_audit_batch_format": "json",
  "db_connection_timeout": 30,
  "db_journal_mode": "WAL",
  "db_cache_size": 10000,
  "db_connection_pool_size": 5,
  "metadata_retention_days": 365,
  "compliance_retention_days": 2555,
  "auto_cleanup_enabled": true,
  "cleanup_schedule": "daily",
  "default_export_format": "json",
  "export_batch_size": 1000,
  "include_metadata_hash": true,
  "encrypt_sensitive_data": true,
  "encryption_key_path": null,
  "audit_all_access": true,
  "compliance_frameworks": [
    "GDPR",
    "FDA",
    "EEOC",
    "FCRA",
    "HIPAA",
    "ISO_13485"
  ],
  "required_metadata_fields": [
    "model_name",
    "stage",
    "event_type",
    "timestamp"
  ],
  "compliance_score_threshold": 0.8,
  "enable_metrics": true,
  "metrics_endpoint": null,
  "alert_on_compliance_failure": true,
  "alert_webhook_url": null
}